<% if previous_question = Question.define_question(params[:type_questions],
                                                   current_user.id,
                                                   @question.id,
                                                   false)[0] %>
  <%= link_to 'Previous', question_path(previous_question,
                                       type_questions: params[:type_questions]),
              id: 'previous', class: 'btn btn-primary',
              remote: true if previous_question %>
<% end %>
